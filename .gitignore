# Wordpress - ignore core, configuration, examples, uploads and logs.
# https://github.com/github/gitignore/blob/main/WordPress.gitignore

# Core
#
# Note: if you want to stage/commit WP core files
# you can delete this whole section/until Configuration.
/wp/wp-admin/
/wp/wp-content/index.php
/wp/wp-content/languages
/wp/wp-content/plugins/akismet
/wp/wp-content/plugins/index.php
/wp/wp-content/themes/index.php
/wp/wp-includes/
/wp/index.php
/wp/license.txt
/wp/readme.html
/wp/wp-*.php
/wp/xmlrpc.php

# Configuration
/wp/wp-config.php

# Example themes
/wp/wp-content/themes/twenty*/

# Example plugin
/wp/wp-content/plugins/hello.php

# Uploads
/wp/wp-content/uploads/

# Log files
/wp*.log
/logs/

# lando and composer files
composer.lock
.env
.lando.yml
/vendor/

# htaccess
/.htaccess

# All plugins
#
# Note: If you wish to whitelist plugins,
# uncomment the next line
/wp/wp-content/plugins
/wp/wp-content/mu-plugins

# All themes
#
# Note: If you wish to whitelist themes,
# uncomment the next line
!/wp/wp-content/themes/nb-tickets/*
wp/wp-content/themes/nb-tickets/node_modules/
